package SelfCheckOut.Exceptions;

/**
 * An exception which is thrown when user inputs a negative tax rate. 
 * It is unlikely for the government to give money to the people, usually it does 
 * the taking ;)
 * 
 * @author Zafer Sawaf
 *
 */

public class InvalidTaxException extends Exception {
	private static final long serialVersionUID = 1L;

	public InvalidTaxException() {
		super();
	}

	public InvalidTaxException(String message) {
		super(message);
	}

	public InvalidTaxException(String message, Throwable cause) {
		super(message, cause);
	}

	public InvalidTaxException(Throwable cause) {
		super(cause);
	}

}
